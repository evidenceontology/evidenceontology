synonym: "restriction landmark genome scanning evidence" EXACT []
synonym: "RLGS evidence" EXACT []
is_a: ECO:0005519 ! DNA detection assay evidence
property_value: IAO:0000234 SIB:PG xsd:string

[Term]
id: ECO:0000090
